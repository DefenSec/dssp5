(in glib.gio

    (block launchdesktop

	   (blockinherit .user.agent.template)

	   (block exec

		  (filecon "/usr/lib/([^/]+/)?glib-2\.0/gio-launch-desktop"
			   file file_context))))

(in user

    (call .glib.gio.launchdesktop.role (role)))

(in wheel

    (call .glib.gio.launchdesktop.role (role)))
