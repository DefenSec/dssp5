(block namespacehelper

       (blockinherit .sys.agent.template)

       (block exec

	      (filecon "/usr/bin/pam_namespace_helper" file file_context))

       (block unit

	      (filecon "/usr/lib/systemd/system/pam_namespace\.service.*" file
		       file_context)

	      (blockinherit .file.unit.template)))
