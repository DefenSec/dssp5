(in php

    (block cli

	   (blockinherit .user.agent.template)

	   (block exec

		  (filecon "/usr/bin/phar[0-9]\.[0-9]\.phar" file file_context)
		  (filecon "/usr/bin/php[0-9]\.[0-9]" file file_context))))

(in user

    (call .php.cli.role (role)))

(in wheel

    (call .php.cli.role (role)))
