(in postfix.flush

    (blockinherit .agent.template)

    (call master.agent (subj exec.file))

    (call .rbacsep.constrained.type (subj))
    (call .rbacsep.usefdsource.type (subj))

    (block exec

	   (filecon "/usr/bin/flush" file file_context)))
